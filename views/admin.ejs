    <form action="/patientregister" method="post">
        <h2>patient register</h2>
        <div class="container">
            <label for="Username"><b>Username</b></label>
            <input type="text" placeholder="Enter Username" name="username" required><br>
            
            <label for="realname"><b>Real name</b></label>
            <input type="text" placeholder="Enter Realname" name="realname" required>
            <br>
            <label for="password"><b>Password</b></label>
            <input type="password" placeholder="Enter Password" name="password" required><br>
            <button type="submit">submit</button>
    
        </div>
    </form>

    <form action="/docregister" method="post">
        <h2>doctor register</h2>
        <div class="container">
            <label for="Username"><b>Username</b></label>
            <input type="text" placeholder="Enter Username" name="username" required><br>
            
            <label for="realname"><b>Real name</b></label>
            <input type="text" placeholder="Enter Realname" name="realname" required>
            <br>
            <label for="password"><b>Password</b></label>
            <input type="password" placeholder="Enter Password" name="password" required><br>
            <label for="yearsOfExp"><b>Years of experience</b></label>
            <input type="text" placeholder="Years of experience" name="yearsOfExp" required><br>
            <label for="specialization"><b>Specialization</b></label>
            <input type="text" placeholder="Specialization" name="specialization" required><br>

            <button type="submit">Submit</button>
    
        </div>
    </form>

</body>
